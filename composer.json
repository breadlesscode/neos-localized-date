    "type": "neos-plugin",
    "name": "breadlesscode/neos-localized-date",
    "require": {
        "neos/flow": "*"
    },
    "autoload": {
        "psr-4": {
            "Breadlesscode\\LocalizedDate\\": "Classes/"
        }
    },
    "extra": {
